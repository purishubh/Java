import java.util.*;
class Main {
	public static void main(String args[]) {
		// Your code goes here
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		for(int i=1; i<=n; i++){

			for(int s=1; s<=n-i; s++){
				System.out.print("-");
			}

			for(int j=1; j<=i; j++){
					System.out.print("*"+ "-" );
			}
			System.out.println();
		}


	}
}
