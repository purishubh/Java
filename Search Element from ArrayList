import java.lang.*;
import java.util.*;

public class Main {

	public static boolean searchElement(ArrayList<Integer> arr, int b){
		int arrSize = arr.size();
		for(int i=0; i<arrSize; i++){
			if(arr.get(i) == b){
				return true;
			}
		}
		return false;
	}

	public static void printElement(ArrayList<Integer> arr, int b){
		//System.out.println("Size: "+arr.size());
		int arrSize = arr.size();
		for(int i=0; i<arrSize; i++){
			//System.out.print(arr.get(i)+" ");
		}
	}

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt();
        System.out.println("T: "+T);
		
        for(int i=1; i<=T; i++){
            ArrayList<Integer> arr = new ArrayList<Integer>();
			int arrLength = sc.nextInt();
			System.out.println("Arr lenght: "+arrLength);

			for(int j=0; j<arrLength; j++){
				arr.add(sc.nextInt());
			}
			System.out.println("Array size: "+arr.size());

			int findVal = sc.nextInt();
			System.out.println("Find value: "+findVal);


			if(searchElement(arr,findVal)){
				System.out.println("Result: "+1);
			}
			else{
				System.out.println("Result: "+0);
			}
			
			printElement(arr,findVal);

        }

		
    }
}
